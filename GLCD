void delay(unsigned int time)
{
    int i,j;
    for(i=0;i<time;i++)
        for(j=0;j<10;j++);
}

void ldelay(unsigned int time)
{
    int i,j;
for(i=0;i<time;i++)
   for(j=0;j<5000;j++);
}

void LCD_strobe(void)
{
 IO1SET = EN;
 delay(5);
 IO1CLR = EN;
 delay(5);
 }
 
 void GLCD_data(unsigned char ch)
 {
   IO1CLR =LCD_Port;
   IO1SET = ch<<LCD_Shift;
   IO1SET = RS;
   LCD_strobe();  
}

void GLCD_cmd(unsigned char ch)
 {
   IO1CLR =LCD_Port;
   IO1SET = ch<<LCD_Shift;
   IO1CLR = RS;
   LCD_strobe();  
}
void GLCD_init()
{
   int i;
   PINSEL0 =0;
   PINSEL1 = 0;
   PINSEL2 = 0;
   
   IODIR1= LCD_Port | RS | EN | CS1 | CS2 | GRST | RW;
   
   IOSET1 =GRST |CS1 | CS2;
   IOCLR1= RW | RS | EN;
   
   for(i=0;i<10;i++)
   GLCD_cmd(0x3F);
   GLCD_cmd(0x40);
   GLCD_cmd(0xB8);
   }
   
   void GLCD_disp(unsigned char *temp1)
   {
   int page,col;
   for(page=0; page<8;page++)
   {
     IO1SET = CS1;
IO1CLR = CS2;
GLCD_cmd(0xB8 | page);
GLCD_cmd(0x40);
for(col=0;col<64;col++)
GLCD_data(temp1[(page*128)+col]);

IO1SET = CS2;
IO1CLR = CS1;
GLCD_cmd(0xB8 | page);
GLCD_cmd(0x40);
for(col=64;col<128;col++)
GLCD_data(temp1[(page*128)+col]);
}}

int main()
{
GLCD_init();

while(1)
{
 GLCD_disp(pic);
ldelay(2000);
	GLCD_disp(pic1);
ldelay(2000);
GLCD_disp(pic2);
     ldelay(2000);
}
}
